{
    "name": "Pming Extension",
    "description": "Made by Hyobin",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["contextMenus", "bookmarks", "tabs", "storage", "activeTab", "topSites", "history"],
    "icons": {
        "16": "icons/icon_16.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },

    "action": {
        "default_title": "pming",
        "default_icon": "icons/icon_16.png"
    },

    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
